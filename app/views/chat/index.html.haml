= content_for :head do
  = javascript_include_tag "#{Cloudsdale.config['url']}:#{Cloudsdale.config['faye_port']}/faye.js"

#chat_sidebar

%ul#chat_menu

#chat_frame
      
#chat_bar
  = form_tag chat_messages_path, remote: true, id: 'new_message' do
    = text_field_tag :message
    = hidden_field_tag :channel, "chat"
    = submit_tag "Send"
    
