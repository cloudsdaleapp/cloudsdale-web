- unless @model.selfReference()
  .chat-message-avatar
    %a.message-user-inspect{ href: "#" }
      - avatarUrl = if @model.get('user').get('avatar') then @model.get('user').get('avatar').thumb else ""
      %img{ src: avatarUrl }
  .chat-message-meta
    %span= @model.timestamp().toString('HH:mm:ss')
  .chat-message-head
    %a.message-user-inspect{ href: "#" }
      = @model.get('user').get('name')
      
%ul.chat-message-drops
.chat-message-content
