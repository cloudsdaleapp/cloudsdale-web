#= require modules/cloud

do ($ = jQuery) ->
  
  class BottomBar
    constructor: (@frame,args) ->
      @faye = args.faye
      @userId = args.userId
      @userName = args.userName
      @userAvatar = args.userAvatar
      @userPath = args.userPath
      
      @clouds = @frame.find('.cloud-list')
      $.each $(@clouds.children()), (key, val) =>
        $(val).cloud
          parent: @
        
  $.fn.bottomBar = ->
    new BottomBar(@,arguments[0])
